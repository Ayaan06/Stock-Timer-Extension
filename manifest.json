{
	"manifest_version": 3,
	"content_scripts": [
		{

			"matches": ["<all_urls>"],

			"js": ["popup.js"],
			"default_title": "Text Finder"
		}
	],
	"background": {
	 "service_worker": "background.js"
  },




	"name": "Save Time",
	"version": "1.0.0",
	"icons": { "128": "Icon_128.png" },
"action":   {
		"default_icon": "Icon.png",
		"default_popup": "popup.html"
	},

	"permissions": [
		"tabs",
		"activeTab",
		"notifications",
		"storage"



	]
}
